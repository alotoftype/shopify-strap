{
  "name": "shopify-strap",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.24.5",
    "colors": "^1.3.0",
    "concurrently": "^3.6.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-better-rollup": "^3.1.0",
    "gulp-changed": "^3.2.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-flatten": "^0.4.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-image": "^4.3.0",
    "gulp-jsonminify": "^1.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-mode": "^1.0.2",
    "gulp-plumber": "^1.2.0",
    "gulp-print": "^5.0.0",
    "gulp-rename": "^1.2.3",
    "gulp-sass": "^4.0.1",
    "gulp-sequence": "^1.0.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svg-sprite": "^1.4.0",
    "gulp-zip": "^4.1.0",
    "path": "^0.12.7",
    "rollup": "^0.59.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-stream": "^1.24.1",
    "shopify-sync": "^0.0.6",
    "url-pattern": "^1.0.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "bootstrap": "^4.1.1",
    "currency-symbol-map": "^4.0.4",
    "loadjs": "^3.5.4",
    "microtip": "^0.2.2",
    "mithril": "^1.1.6",
    "money": "^0.2.0",
    "picturefill": "^3.0.2",
    "siema": "^1.5.1",
    "simplestatemanager": "^4.1.1",
    "store": "^2.0.12",
    "turbolinks": "^5.1.1"
  },
  "scripts": {
    "store": "store",
    "develop": "concurrently \"gulp --development --silent\" \"store theme watch --target=development\"",
    "production": "gulp production --silent --production && gulp zip --silent && yarn build",
    "build": "gulp build --silent --development",
    "watch": "store theme watch",
    "icons": "gulp icons --silent"
  },
  "config": {
    "scripts": {
      "bundle": {
        "input": "bundle.js",
        "name": "Bundle",
        "base": "./src/assets/scripts/",
        "format": "iife",
        "watch": [
          "./src/assets/scripts/**/*.js"
        ]
      },
      "include": [
        "./src/assets/scripts"
      ],
      "dest": "./theme/assets/"
    },
    "styles": {
      "src": [
        "./src/assets/styles/stylesheet.scss"
      ],
      "watch": [
        "./src/assets/styles/**/*.scss"
      ],
      "include": [
        "./node_modules/",
        "./node_modules/bootstrap/scss/"
      ],
      "dest": "./theme/assets/"
    },
    "views": {
      "customers": {
        "src": [
          "./src/views/customers/*.liquid"
        ],
        "dest": "./theme/templates/customers/",
        "watch": [
          "./src/views/customers/*.liquid"
        ]
      },
      "layout": {
        "src": [
          "./src/views/layout/*.liquid"
        ],
        "dest": "./theme/layout/",
        "watch": [
          "./src/views/layout/*.liquid"
        ]
      },
      "sections": {
        "src": [
          "./src/views/sections/*.liquid"
        ],
        "dest": "./theme/sections/",
        "watch": [
          "./src/views/sections/*.liquid"
        ]
      },
      "snippets": {
        "src": [
          "./src/views/snippets/layout/*.liquid",
          "./src/views/snippets/collection/*.liquid",
          "./src/views/snippets/index/*.liquid",
          "./src/views/snippets/pages/*.liquid",
          "./src/views/snippets/cart/*.liquid",
          "./src/views/snippets/product/*.liquid"
        ],
        "dest": "./theme/snippets/",
        "watch": [
          "./src/views/snippets/**/*.liquid"
        ]
      },
      "templates": {
        "src": [
          "./src/views/templates/*.liquid"
        ],
        "dest": "./theme/templates/",
        "watch": [
          "./src/views/templates/*.liquid"
        ]
      }
    },
    "icons": {
      "src": [
        "./src/assets/icons/*.svg"
      ],
      "dest": "theme/sections/",
      "watch": [
        "./src/assets/icons/*.svg"
      ]
    },
    "images": {
      "src": [
        "./src/assets/images/**/*"
      ],
      "dest": "./theme/assets/",
      "watch": [
        "./src/assets/images/hero/*"
      ]
    },
    "json": {
      "config": {
        "src": [
          "./src/config/*.json"
        ],
        "dest": "./theme/config/",
        "watch": [
          "./src/config/*.json"
        ]
      },
      "locales": {
        "src": [
          "./src/locales/*.json"
        ],
        "dest": "./theme/locales/",
        "watch": [
          "./src/locales/*.json"
        ]
      }
    }
  }
}
